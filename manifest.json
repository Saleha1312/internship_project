{
  "manifest_version": 3,
  "name": "Real-Time Page Data Extractor",
  "version": "1.1",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "http://localhost:8000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}